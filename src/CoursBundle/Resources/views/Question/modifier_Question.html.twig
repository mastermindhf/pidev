{% extends 'Back/backE.html.twig' %}
{% block body %}
    <table class="table table-striped">
        {{ form_start(form) }}
        <tr>

            <td>{{ form_label(form.question) }}</td>
            <td>{{ form_widget(form.question) }}</td>

        </tr>
        <tr>
            <td>{{ form_label(form.cours) }}</td>
            <td>{{ form_widget(form.cours) }}</td>

        </tr>
        <tr>
            <td>{{ form_label(form.date) }}</td>
            <td>{{ form_widget(form.date) }}</td>
            <td>{{ form_errors(form.date) }}</td>

        </tr>

        <td></td>
        <td>{{ form_widget(form.Modifier, { 'attr': {'class': 'btn btn-info  '}} ) }}</td>

        {{ form_end(form) }}
    </table>


{% endblock %}