{% extends 'Back/backE.html.twig' %}
{% block body %}
<table class="table table-striped">
    <tr align="center"  >
        <td><b>ID</b></td>
        <td><b>Libelle</b></td>
        <td colspan="2"><b>Action</b></td>
    </tr>
    {% for c in Niveau %}
    <tr align=" center" >
        <td>{{c.id}}</td>
        <td>{{c.Libelle}}</td>
        <td><a href="{{ path('_modifier_niveau',{'id':c.id}) }}"><i class="fa fa-edit" style="color:#2e6da4 " ></i></a></td>
        <td><a href="{{ path('_supprimer_niveau',{'id':c.id}) }}"><i class="fa fa-trash" style="color: #ff6c5f"></i></a></td>
    </tr>
    {% endfor %}
</table>

{% endblock %}